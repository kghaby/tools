#!/bin/bash
set -e 
#/scratch/ksg225/projects/insulin/insAB/monomer/simulation/4-prod/*K/converged_analysis

gnuplot -persist << EOF
	
#Alpha
	set term x11 1	
        set font '{/Times-New-Roman=18 a}' 
        set xlabel 'Time (Frames)'
        set ylabel 'Fraction of Alpha-Helicity'  
	set key b r
        #set yrange [0:0.9]
        set title 'Alpha Helicity of Entire Monomer Over Time in CAGE' 
plot "/scratch/ksg225/projects/insulin/insAB/monomer_CAGE/0.0XM/simulation/4-prod/253K/converged_analysis/secstruct_1us/all.dat" using 1:2 every 1000 w l title "253 K",\
"/scratch/ksg225/projects/insulin/insAB/monomer_CAGE/0.0XM/simulation/4-prod/294K/converged_analysis/all.dat" using 1:2 every 1000 w l title "294 K",\
"/scratch/ksg225/projects/insulin/insAB/monomer_CAGE/0.0XM/simulation/4-prod/310K/converged_analysis/all.dat" using 1:2 every 1000 w l title "310 K",\
"/scratch/ksg225/projects/insulin/insAB/monomer_CAGE/0.0XM/simulation/4-prod/350K/converged_analysis/all.dat" using 1:2 every 1000 w l title "350 K"

#Total Helicity
        set term x11 2  
        set font '{/Times-New-Roman=18 a}' 
        set xlabel 'Time (Frames)'
        set ylabel 'Fraction of Total-Helicity'  
        set key b r
        #set yrange [0:0.9]
	set title 'Total Helicity of Entire Monomer Over Time in CAGE' 
plot "/scratch/ksg225/projects/insulin/insAB/monomer_CAGE/0.0XM/simulation/4-prod/253K/converged_analysis/all.dat" using 1:(column(2)+column(6)) every 1000 w l title "253 K",\
"/scratch/ksg225/projects/insulin/insAB/monomer_CAGE/0.0XM/simulation/4-prod/294K/converged_analysis/all.dat" using 1:(column(2)+column(6)) every 1000 w l title "294 K",\
"/scratch/ksg225/projects/insulin/insAB/monomer_CAGE/0.0XM/simulation/4-prod/310K/converged_analysis/all.dat" using 1:(column(2)+column(6)) every 1000 w l title "310 K",\
"/scratch/ksg225/projects/insulin/insAB/monomer_CAGE/0.0XM/simulation/4-prod/350K/converged_analysis/all.dat" using 1:(column(2)+column(6)) every 1000 w l title "350 K"

	pause -1

EOF

