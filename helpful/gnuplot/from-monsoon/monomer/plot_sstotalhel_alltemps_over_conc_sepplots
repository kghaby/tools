        conc = "0 0.085 1.002"
        set bars 4
plot "totalhelavg_AAGE_253.dat"   using (word(conc,int(column(0))+1)+0.0):1 with linespoints title "253" linestyle 1, "" using (word(conc,int(column(0))+1)+0.0):1:3 with yerrorbars linetype 1 notitle,\
"totalhelavg_AAGE_294.dat"  using (word(conc,int(column(0))+1)+0.0):1 with linespoints title "294" linestyle 2, "" using (word(conc,int(column(0))+1)+0.0):1:3 with yerrorbars linetype 2 notitle,\
"totalhelavg_AAGE_310.dat"  using (word(conc,int(column(0))+1)+0.0):1 with linespoints title "310" linestyle 3, "" using (word(conc,int(column(0))+1)+0.0):1:3 with yerrorbars linetype 3 notitle,\
"totalhelavg_AAGE_350.dat"   using (word(conc,int(column(0))+1)+0.0):1 with linespoints title "350" linestyle 4, "" using (word(conc,int(column(0))+1)+0.0):1:3 with yerrorbars linetype 4 notitle

#EMIM_TFN
        set term x11 2  
        set font '{/Times-New-Roman=18 a}' 
        set xlabel 'Concentration of Sets of EMIM_TFN (M)'
        set ylabel 'Fraction of Total-Helicity'  
        set key b r
        #set yrange [0:1]
        set title 'Total Helicity vs EMIM_TFN Concentration' 
        set xrange [-0.01:1.015]
        conc = "0 0.090 1.005"
