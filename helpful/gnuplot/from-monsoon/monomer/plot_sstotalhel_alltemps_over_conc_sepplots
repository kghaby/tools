gnuplot -persist << EOF
	
#CAGE
	set term x11 0
        set font '{/Times-New-Roman=18 a}' 
        set xlabel 'Concentration of Sets of CAGE (M)' 
        set ylabel 'Fraction of Total-Helicity' 
	set key b r
        #set yrange [0:1]
        set title 'Total Helicity vs CAGE Concentration' 
	set xrange [-0.01:1.013]
	conc = "0 0.072 1.003"
	set bars 4
plot "totalhelavg_CAGE_253.dat"   using (word(conc,int(column(0))+1)+0.0):1 with linespoints title "253" linestyle 1, "" using (word(conc,int(column(0))+1)+0.0):1:3 with yerrorbars linetype 1 notitle,\
"totalhelavg_CAGE_294.dat"  using (word(conc,int(column(0))+1)+0.0):1 with linespoints title "294" linestyle 2, "" using (word(conc,int(column(0))+1)+0.0):1:3 with yerrorbars linetype 2 notitle,\
"totalhelavg_CAGE_310.dat"  using (word(conc,int(column(0))+1)+0.0):1 with linespoints title "310" linestyle 3, "" using (word(conc,int(column(0))+1)+0.0):1:3 with yerrorbars linetype 3 notitle,\
"totalhelavg_CAGE_350.dat"   using (word(conc,int(column(0))+1)+0.0):1 with linespoints title "350" linestyle 4, "" using (word(conc,int(column(0))+1)+0.0):1:3 with yerrorbars linetype 4 notitle

#AAGE
        set term x11 1
        set font '{/Times-New-Roman=18 a}' 
        set xlabel 'Concentration of Sets of mCAGE (M)' 
        set ylabel 'Fraction of Total-Helicity'  
